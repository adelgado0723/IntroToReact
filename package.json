{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "An app to teach you to write React",
  "main": "AdoptMe/src/App.js",
  "scripts": {
    "test": "jest --silent",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "format": "prettier --write \"AdoptMe/src/**/*.{js,jsx}\"",
    "lint": "eslint \"AdoptMe/src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel AdoptMe/src/index.html"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!petfinder-client).+"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adelgado0723/IntroToReact.git"
  },
  "keywords": [
    "React",
    "JavaScript"
  ],
  "author": "Andy Delgado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adelgado0723/IntroToReact/issues"
  },
  "homepage": "https://github.com/adelgado0723/IntroToReact#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@reach/router": "^1.2.1",
    "emotion": "^10.0.9",
    "petfinder-client": "^1.0.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^10.0.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.0.3",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
